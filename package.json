    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "eslint --fix ./src ./plugins",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest --colors",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.11.1",
    "gatsby": "2.25.4",
