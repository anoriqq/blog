    "jest": "26.0.1",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "react-test-renderer": "16.13.1"
  },
  "keywords": [
    "gatsby"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn type-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-pattern '!/.eslintrc.js' --ignore-pattern '/@types/graphql-types.d.ts'"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier-package-json --write"
    ]
  }
}
